{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/landing.html"
    },
    {
      "src": "/video-player",
      "dest": "/video-player.html"
    },
    {
      "src": "/register",
      "dest": "/register.html"
    }
  ]
}
